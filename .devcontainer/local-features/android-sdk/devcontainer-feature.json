{
  "name": "Android SDK",
  "id": "android-sdk",
  "version": "1.0.0",
  "description": "Installs Android SDK and NDK",
  "options": {
    "ANDROID_SDK_TOOLS_VERSION": {
      "type": "string",
      "default": "9477386",
